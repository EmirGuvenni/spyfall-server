{
	"compilerOptions": {
	  "target": "ESNext",
	  "module": "commonjs",
	  "rootDir": "./src",
	  "outDir": "./dist",
	  "esModuleInterop": true,
	  "resolveJsonModule": true,
	  "strict": true,
	  "baseUrl": "./src",
	  "paths": {
		  "@class/*": ["./classes/*"],
		  "@interface/*": ["./interfaces/*"],
		  "@config/*": ["./config/*"],
		  "@index": ["./index"],
		  "@events/*": ["./events/*"]
	  }
	}
}